      - strings:
        - '*tutorial_start'
      - strings:
        - 
        - Player
        - 
        - 
        - 
        - 
        - 
        - "Ugh\u2026 my head. Where\u2026 am I? I feel like I'm burning up\u2026"
      - strings:
        - 
        - Player
        - 
        - 
        - 
        - 
        - 
        - "The restaurant. I gotta get back\u2026 to the restaurant."
      - strings:
        - EndScenario
